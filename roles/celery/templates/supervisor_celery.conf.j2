[program:celery_worker]
command={{ celery_scripts_dir }}/celery_worker_start

autostart=true
autorestart=true

user={{ celery_user }}

stdout_logfile={{ celery_log_file }}
redirect_stderr = true

[program:celery_beat]
command={{ celery_scripts_dir }}/celery_beat_start

autostart=true
autorestart=true

user={{ celery_user }}

stdout_logfile={{ celery_log_file }}
redirect_stderr = true
